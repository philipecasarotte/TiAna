<h1>Listing Images of Gallery "<%= parent_object.name %>"</h1>
<br />
<table>
  <tr>
    <th>Image</th>
  </tr>
  <% @images.each do |image| %>
    <tr>
      <td><%= image_tag image.image.url(:thumb) %></td>
      <td><%= link_to "Editar", edit_object_url(image) %></td>
      <td><%= link_to "Deletar", object_url(image), :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>
<br />
<%= link_to "New Imagem", new_object_url %> | <%= link_to "Reorder", reorder_admin_image_gallery_images_path(parent_object) %> | <%= link_to "Back", admin_image_galleries_path %>